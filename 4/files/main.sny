@require({ random: 'random' })
fun pick(items: list) {
	return items.at(random::new(0 to items.len()))
}

class range {
	private const T = Not<(-infinity), infinity> + num
	static class CharRange {
		private const T = str::char
		static fun new(x: T, y: T) {}
	}
	private {
		const x: T;
		const y: T;
	}
	static fun new(self: Self, x: T, y: T) {
		self += { x, y }
	};
	fun set_inclusive(inclusive: bool as IsInclusive);
}

const int: range = range<str::char>::new()

