fun require(f, ...modules: str[]) -> Copy<f> {
	fun handler(...args) {
		for module in modules {
			unless exists(module) {
				throw ReferenceError::new('module ${module} required')
			}
		}
		f(...args)
	}
}

@require('random::new as get_random')
fun pick_an_item(items: list) {
	return items.at(random::new(0 to items.len()))
}

