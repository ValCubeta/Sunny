mod pretty {
  const RED_START := "\u{1b}[3m"
  mixin Colorize where Self: Display {
    fun red() => f"{RED_START}{self}{COLOR_END}"
    fun bold() => f"{BOLD_START}{self}{BOLD_END}"
  }
}

mixin Number where Self: Add + Sub + Mul + Div {
  fun to(end: Self) => Range::new(self, end)
}

fun main() {
  for i in 0.to(10) {
    println(i)
  }
}
