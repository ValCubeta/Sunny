# comment

trait Any {}

trait Init {
  fn new()
}

fun create<T>(p: T) -> T
where
  T: Init
{
  return T::new()
}

struct Point {
  pub [x, y]: u8  
}

extend Point {
  impl Display, Debug
  pub fun new(x = 0, y = 0) {
    return Point { x, y }
  }
}

fun main(_args: String[]) {
  let point = create<Point>()
  println(point)
  assert point.x == 0
}